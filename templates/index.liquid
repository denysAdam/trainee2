{% for product in collections.all.products %}
  <h2>{{ product.title }}</h2>
  <p>{{ product.description }}</p>
  <p>Цена: {{ product.price | money }}</p>
  <p>Цена: {{ product.variants  }}</p>
{% endfor %}

{% for product in collections.all.products %}
  <h2>{{ product.title }}</h2>
  <pre>{{ product | json }}</pre>
{% endfor %}
